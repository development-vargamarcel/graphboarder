    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.13",
    "drizzle-kit": "^0.22.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "mdsvex": "^0.11.2",
    "prettier": "^3.3.2",
